boundary        s s p
units           metal
atom_style      atomic
variable    	T equal 300

#variable str equal E20

timestep        .001


read_data   struct

pair_style eam/alloy

pair_coeff * * CuNi.eam.alloy Ni Cu
neighbor        2.0 bin
neigh_modify    every 1




thermo 1000
reset_timestep  0
dump min all atom 1000 dump.min
min_style 	cg 
minimize 1e-25 1e-25 5000 10000
undump 		min 

velocity        all create $T 11245 dist gaussian

thermo 1000


reset_timestep  0




dump nve all atom 50000 dump.nve
fix             1 all nve
fix 		2 all langevin $T $T 0.1 699483
run             50000
undump 		nve
unfix 		1
unfix 		2



dump npt all atom 50000 dump.npt
fix 		1 all npt temp $T $T 0.1 z 1.0 1.0 .1 drag 0.3
run 		100000
undump 		npt                              
unfix 		1





dump nvt all atom 50000 dump.nvt
fix             1 all nvt temp $T $T 0.1 drag 0.3
run             10000
undump 		nvt                              
unfix           1



compute 	csym all centro/atom 12
compute 	2 all stress/atom NULL
compute 	mytemp all temp
compute 	11 all reduce sum c_2[1]
compute 	12 all reduce sum c_2[2]
compute 	13 all reduce sum c_2[3]
compute 	14 all reduce sum c_2[4]
compute 	15 all reduce sum c_2[5]
compute 	16 all reduce sum c_2[6]






variable 	tmp equal "lz"
variable 	L0 equal ${tmp}
print 		"Initial Length, L0: ${L0}"



reset_timestep 	0

fix             3 all nvt temp $T $T 0.1 drag 0.3
fix 		4 all deform 1 z erate 0.001 units box 



variable strain 	equal "(lz - v_L0)/v_L0"
variable 		p1 equal "v_strain"
variable 		p4 equal "-pzz/10000"
variable 		sf equal "v_p4*1.27324"
dump final all atom 50000 dump.final


fix def_print2 all print 1000 "${p1} ${sf} " file result_final.txt screen no

run 300000